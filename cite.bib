	volume = {266},
	shorttitle = {Deep–diving behaviour of the northern bottlenose whale, {Hyperoodon} ampullatus ({Cetacea}},
	url = {https://royalsocietypublishing.org/doi/10.1098/rspb.1999.0688},
	doi = {10.1098/rspb.1999.0688},
	abstract = {Using suction–cup attached time–depth recorder/VHF radio tags, we have obtained the first diving data on northern bottlenose whales (Hyperoodon ampullatus), the first such data on any species within the family Ziphiidae. Two deployments in 1997 on northern bottlenose whales in a submarine canyon off Nova Scotia demonstrated their exceptional diving ability, with dives approximately every 80 min to over 800 m (maximum 1453 m), and up to 70 min in duration. Sonar traces of non–tagged, diving bottlenose whales in 1996 and 1997 suggest that such deep dives are not unusual. This combined evidence leads us to hypothesize that these whales may make greater use of deep portions of the water column than any other mammal so far studied. Many of the recorded dives of the tagged animals were to, or close to, the sea floor, consistent with benthic or bathypelagic foraging. A lack of correlation between dive times and surface intervals suggests that the dives were predominately aerobic.},
	number = {1420},
	urldate = {2025-03-22},
	journal = {Proceedings of the Royal Society of London. Series B: Biological Sciences},
	author = {Hooker, Sascha K. and Baird, Robin W.},
	year = {1999},
	note = {Publisher: Royal Society},
	keywords = {Ziphiidae, diving, foraging, Hyperoodon ampullatus, northern bottlenose whale, time—depth recorder},
	pages = {671--676},
	file = {Full Text:C\:\\Users\\micha\\Zotero\\storage\\X7KGVEVW\\Hooker and Baird - 1999 - Deep–diving behaviour of the northern bottlenose whale, Hyperoodon ampullatus (Cetacea Ziphiidae).pdf:application/pdf},
}

@article{jeanniard-du-dot_motes_2017,
