@article{werner_optimal_1974,
	title = {Optimal foraging and the size selection of prey by the bluegill sunfish (\textit{{Lepomis} {Macrochirus}})},
	volume = {55},
	copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
	issn = {0012-9658, 1939-9170},
