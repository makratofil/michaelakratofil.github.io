	title = {False killer whales and fisheries interactions in {Hawaiian} waters: {Evidence} for sex bias and variation among populations and social groups},
	volume = {31},
	issn = {0824-0469, 1748-7692},
	shorttitle = {False killer whales and fisheries interactions in {Hawaiian} waters},
