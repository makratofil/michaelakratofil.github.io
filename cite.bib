@article{pacher_evidence_2024,
	title = {Evidence for a by‐product mutualism in a group hunter depends on prey movement state},
	volume = {38},
	issn = {0269-8463, 1365-2435},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/1365-2435.14638},
	doi = {10.1111/1365-2435.14638},
	abstract = {Abstract
            
              
                
                  Group‐hunting animals have been shown to engage their prey in long pursuits (kms) to increase capture success, but the evidence for this is limited to a few terrestrial species. This predation strategy may be effective in the open‐ocean, where group‐hunts are characterised by large predator–prey size ratios and there are few places for prey to hide.
                
                
                  However, recording the attack rates, capture rates and capture success probabilities of grouping predators during long‐distance pursuits through the open ocean, remains a significant challenge.
                
                
                  
                    Here we use a combination of underwater and aerial video to assess the attack and capture behaviour of striped marlin,
                    Kajikia audax
                    , in relation to the movement of their schooling prey (‘stationary’ and ‘mobile’ movement states) during group hunts.
                  
                
                
                  We found that the attack rate was higher when the prey school was ‘stationary’ and, during these times, individual marlin could make longer sequences of uninterrupted attacks on the prey school, effectively monopolising the prey resource for short periods over conspecifics.
                
                
                  When the prey school was ‘mobile’, the attack pattern changed and individual marlin alternated their attacks on the prey school more frequently. Capture success probability (likelihood of capture per attack) was not affected by prey movement state. However, attacks on ‘mobile’ schools resulted in more prey fish becoming isolated from the school, and these isolated prey were easily captured—not by the attacker but by conspecifics swimming nearby.
                
                
                  
                    The likelihood of a prey becoming isolated from a ‘mobile’ school was predicted by prey school speed, with a 0.1 m s
                    −1
                    increase leading to an 18.01\% increase in the chance of isolation.
                  
                
                
                  Altogether this meant the capture rate at the group level was highest when the prey school was ‘mobile’ rather than ‘stationary’ because the predators that were pursuing the ‘mobile’ prey school had an increased availability of isolated and easy to capture prey.
                
                
                  This mechanism provides evidence for a by‐product mutualism selecting for conspecific group formation and we discuss our findings in consideration of the social structure of the hunting group and the type of prey.
                
              
            
            
              Read the free
              Plain Language Summary
              for this article on the Journal blog.},
	language = {en},
	number = {10},
	urldate = {2025-03-22},
	journal = {Functional Ecology},
	author = {Pacher, K. and Krause, J. and Bartashevich, P. and Romanczuk, P. and Bideau, P. and Pham, D. and Burns, A. L. and Deffner, D. and Dhellemmes, F. and Binder, B. and Boswell, K. M. and Galván‐Magaña, F. and Domenici, P. and Hansen, M. J.},
	year = {2024},
	pages = {2123--2138},
	file = {PDF:C\:\\Users\\micha\\Zotero\\storage\\ZXZX3WLX\\Pacher et al. - 2024 - Evidence for a by‐product mutualism in a group hunter depends on prey movement state.pdf:application/pdf},
}

